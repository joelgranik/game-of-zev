{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable",
                "Access-Control-Allow-Origin": "*"
            },
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)\\.(?:mp3|js|css|jpg|png|ico)$",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            },
            "dest": "/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
} 