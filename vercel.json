{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": { "cache-control": "public, max-age=31536000, immutable" },
            "continue": true
        },
        {
            "handle": "filesystem"
        }
    ]
} 